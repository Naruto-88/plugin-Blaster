{
  "name": "wp-update-monitor",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "start": "pnpm -r start",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "db:migrate": "pnpm --filter @nsm/db prisma migrate deploy",
    "db:seed": "pnpm --filter @nsm/db exec dotenv -e ../../.env -- tsx src/seed.ts",
    "db:admin-reset": "pnpm --filter @nsm/db run admin-reset",
    "db:create-admin": "pnpm --filter @nsm/db run create-admin"
  }
}
